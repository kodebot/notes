{
    "saveAndRun": {
        "commands": [
            {
                "match": "\\.*",
                "isAsync": true,
                "cmd": "cd ${workspaceFolder} && ${workspaceFolder}/.auto/commit.sh",
            }
        ]
    },
    "files.autoSave": "off",
    "excalidraw.image": {
        "exportScale": 1,
        "exportWithBackground": true,
        "exportWithDarkMode": true
    },
    "github.copilot.enable": {
        "*": false,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    },
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#58be92",
        "activityBar.background": "#58be92",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#8747b8",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#58be92",
        "statusBar.background": "#40a378",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#327e5d",
        "statusBarItem.remoteBackground": "#40a378",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#40a378",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#40a37899",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#40a378"
}